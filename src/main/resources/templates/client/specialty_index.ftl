<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"><head>    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">	<title><#if setting??>${setting.seoTitle!''}-</#if>特产商城</title>	<meta name="keywords" content="<#if setting??>${setting.seoKeywords!''}</#if>">	<meta name="description" content="<#if setting??>${setting.seoDescription!''}</#if>">	<meta name="copyright" content="<#if setting??>${setting.copyright!''}</#if>">    </head><link rel="shortcut icon" href="/client/images/tianya.ico"><link rel="stylesheet" type="text/css" href="/client/css/base.css">    <link rel="stylesheet" type="text/css" href="/client/css/common.css">    <link rel="stylesheet" type="text/css" href="/client/css/index.css">    <link rel="stylesheet" type="text/css" href="/client/css/rich_css.css">    <link rel="stylesheet" type="text/css" href="/client/css/f_specialty.css">    <link rel="stylesheet" type="text/css" href="/client/css/home.css">    <script src="/client/js/jquery-1.11.0.js" type="text/javascript"></script>    <script src="/client/js/f_aboutjs.js" ype="text/javascript"></script>    <script src="/client/js/rich_lee.js" type="text/javascript"></script>    <script type="text/javascript">        window.onload = function () {        	banner_go('my');//焦点图            act_nav();            $(".hot_list .a_img").hover(function () {                    $(this).find(".info").stop(false, true).slideDown(500);                },function () {                    $(this).find(".info").stop(false, true).slideUp(500);                });            $("#sub_nav a").click(function(){                $("#sub_nav a").removeClass("on");                $(this).addClass("on");                $("#subcontent > div").css({'display':'none'});                var num = $(this).index()/2;                $("#subcontent").children("div:eq("+num+")").css({'display':'block'});            });        };    </script><body><#include "/client/common_header.ftl" /><div class="index_banner">	<div class="banner_scrool">		<#if big_scroll_ad_list7??>			<#list big_scroll_ad_list7 as item>				<a href="${item.linkUri!''}"><img src="${item.fileUri!''}" /> </a>			</#list>		</#if>	</div>	<div class="move_btn">		<ul>			<#if big_scroll_ad_list7??>				<#list big_scroll_ad_list7 as item>					<li><a></a></li>				</#list>			</#if>		</ul>	</div></div><!--**********************中间内容Start*************************--><div class="wrapper">    <div class="common_wrap">        <h1>特价</h1>        <div class="specialprice_list">	        <#if goodsss_page??>	        <#list goodsss_page.content as item>	            <a href="/detail/${item.id?c}" class="a_img a_smallimg" title="${item.title!''}">	                <img src="${item.coverImageUri!''}">		                <div class="info">	                    <span>${item.title!''}</span>	                    <label><font>￥${item.salePrice!0}元</font> /${item.shipCompany!""}<#--<#if item?? && item.categoryIdTree?contains("[31]")||item.categoryIdTree?contains("[32]")> /500g<#elseif item.categoryIdTree?contains("[33]")> /盒或瓶<#elseif item.categoryIdTree?contains("[34]")>/盒 <#else>/件</#if>--></label>	                </div>	            </a>	        </#list>	        </#if>        </div><#--<a href="#" style="float: right;"><label style="color: #3399ff;">查看更多特价商品...</a>-->    </div>    <div class="common_wrap">        <h1>热门</h1>        <div class="sub_nav" id="sub_nav">            <#if specialty_cat_list??>				<#list specialty_cat_list as item>					<a href="/list/${item.id?c}">${item.title!''}</a>				</#list>			</#if>        </div>        <div class="total_warp" id="subcontent">            <div class="hot_list" style="display: block;">            	<#if hot_specialty_page??>				<#list hot_specialty_page.content as item>		            <a href="/detail/${item.id?c}" class="a_img" title="${item.title!''}">		                <img src="${item.coverImageUri!''}">				                <div class="info">		                    <span>${item.title!''}</span>		                    <label><font>￥<#if item.salePrice??>${item.salePrice?string("0.00")}</#if></font>/${item.shipCompany!""}</label>		                </div>		            </a>		        </#list>		        </#if>            </div><#--<a href="#" style="float: right;"><label style="color: #3399ff;">查看更多热门商品...</a>-->        </div>    </div>    <span class="clear"></span></div><!--**********************中间内容End*************************--><#include "/client/common_footer.ftl" /></body></html>