<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <title>签证详情页</title>    <script type="text/javascript">        // 日期 格式为yyyy-MM-dd的字符串	function ymdCompare(date1, date2){		var ymd1 = date1.replace(/-/g, "");		var ymd2 = date2.replace(/-/g, "");		if(parseInt(ymd1) - parseInt(ymd2) > 0){			return 1;		}else if(parseInt(ymd1) - parseInt(ymd2) < 0){			return -1;		}else{			return 0;		} 	}    	function submitDate(){    	var c = $("#calendar").val();    	var c_to_num = 0;		var c_arrary = c.split("-");		for(var i=0; i < c_arrary.length; i ++){			c_to_num = c_to_num + parseInt(c_arrary[i]);		}		var now_date = new Date();		var year = now_date.getFullYear();		var month = now_date.getMonth() + 1;		if(month < 10){			month = "0" + month;		}		var day = now_date.getDate();		if(day < 10){			day = "0" + day;		}		var ymd = year + "-" + month + "-" + day;    			if(c == null || c == "" || c == "点击选择时间"){			$("#timeError").css("color", "red");			$("#timeError").text("请选择出发日期");			return;		}else if(ymdCompare(c, ymd) < 0){	$("#timeError").css("color", "red");	$("#timeError").text("请选择未来日期");	return;	}else{	    		window.location='/visa/${goods.id?c}?date='+document.getElementById('calendar').value;	    }    }    </script>    </head><link rel="shortcut icon" href="/client/images/tianya.ico"><link rel="stylesheet" type="text/css" href="/client/css/base.css">    <link rel="stylesheet" type="text/css" href="/client/css/common.css">    <link rel="stylesheet" type="text/css" href="/client/css/f_visa_detail.css">    <!--价格日历控件样式-->    <link href="/client/js/DatePicker/css/datepicker.css" rel="stylesheet" />    <script src="/client/js/jquery-1.11.0.js" type="text/javascript"></script>    <script src="/client/js/rich_lee.js" type="text/javascript"></script>    <script src="/client/js/f_aboutjs.js" type="text/javascript"></script>    <script type=text/javascript src="/mag/js/WdatePicker.js"></script><!--价格日历控件-->    <script src="/client/js/DatePicker/js/zlDate.js"></script>    <script type="text/javascript">        window.onload = function () {            act_nav();            //签证详情a锚点效果Start            $('#d_tabs').divfixed({"top":"0"});            $('.d_tabs a').on('click',function(){                // a锚点效果                var posTop_num = $(this).index();                var posTop = $('div[tag_id="'+ posTop_num +'"]').offset().top-72;                $(window).scrollTop(posTop);                //当前高亮                $('.d_tabs a').removeClass('active');                $(this).addClass('active');            });            //滚动sub 高亮            $('div[tag_id]').each(function(index,ele){                $(window).bind('scroll',function(){                    if($(window).scrollTop() >= $(ele).offset().top-72 && $('html,body,document').scrollTop() <= ($(ele).offset().top-72)+$(ele).outerHeight()+$(ele).next().outerHeight()) {                        $('.d_tabs a').removeClass('active');                        $('.d_tabs a').eq(index).addClass('active');                    }                });            });            //签证详情a锚点效果End        }        function click_calender(){            $.get("../js/DatePicker/date.json",function(data) {                pickerEvent.setPriceArr(eval(data));                pickerEvent.Init("calendar");            });        }                $(document).ready(function(){	$("#button1").click(function(){		$.ajax({			type: "post",            url: "/user/collect",                           data: {"goodsId": ${goods.id!c}},            dataType: "json",            success: function (data) {                 if (data.code == 0){                	alert("收藏成功！");                    window.location.reload();                }else if(data.code == 3){                	alert("请先登录！");                	window.location.href="/login";                }else{                	alert(data.msg);                	window.location.reload();                }                 }		});	});});    </script><body><#include "/client/common_header.ftl" /><!-- ********************中间内容Start******************** --><div class="wrapper" style="margin-top:50px;">    <!-- 签证详情-->    <div class="visa_detail">        <div class="v_d_img">        	<li>				<a href="javascript:;">					<img src="${goods.coverImageUri!''}" >				</a>			</li>		</div>        <div class="v_d_info">            <h1>${goods.title!''}</h1>            <div class="in_infos">                <span>                    <label>办理天数：${goods.groupSaleLeftNumber!''}个工作日</label>                    <label>逗留天数：${goods.groupSaleHundredLeftNumber!''}天</label>                </span>                <span>                    <label>有效期限：${goods.totalGift!''}天</label>                    <label>出签率：${goods.shipCompany!''}</label>                </span>                <span>                    <label>总费用：${goods.salePrice?string("0.00")}元</label>                </span>            </div>            <div class="handing_warp">                <div class="choose_date">                    <label>选择出行日期：</label>                    <!--<input type="text" class="timepicker" id="timepicker">-->                    <input type="text" id="calendar" class="timepicker" name="calendar" readonly="readonly" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd',lang:'zh-cn'})" placeholder="点击选择时间"/><span id="timeError"></span>                </div>                <div class="handingbtn_warp">                    <!--<label><a onclick="window.location='/visa/${goods.id?c}?date='+document.querySelector('#calendar').value;" class="btn_hand_now">立即办理</a>-->                    <label><a onclick="submitDate()" class="btn_hand_now">立即办理</a> <a href="javascript:;" id="button1" class="btn_collection">收藏</a> </label>                    	                </div>            </div>        </div>    </div>    <!-- 签证详细介绍-->    <div class="v_description">        <div class="v_infosite">            <div class="d_tabs" id="d_tabs" style="position: static; left:0; top: 0px;">                <a href="javascript:;" class="active">签证材料</a><span></span>                <a href="javascript:;" class="">办证流程</a><span></span>                <a href="javascript:;" class="">费用说明</a><span></span>                <a href="javascript:;" class="">订购须知</a>            </div>            <div class="z_title" tag_id="0">                <h2>签证材料</h2>            </div>            <div class="z_content">                <table cellpadding="1">                    <tr>                        <td width="73%">                            <div class="border_bottom_div">                                <p>用于北京大使馆（面向在北京、新疆、西藏、青海、甘肃、宁夏、内蒙古、黑龙江、吉林、辽宁、天津、山西、陕西、河北、山东、河南、湖北、江西、湖南、四川、重庆、贵州以及云南省居住的申请人）。</p>                            </div>                            <div class="border_bottom_div">                                <div class="border_left_div">                                    <label>核对表</label>                                </div>                                <div class="border_right_div">                                    <div class="border_bottom_div">                                        <label>个人旅游签证申请</label>                                    </div>                                    <div class="border_bottom_div">                                        <label>商务签证申请（由中意商会与意中基金会成员公司邀请的商业人士或中意商会成员公司的职员）</label>                                    </div>                                    <div>                                        <label>商务签证申请（自由贸易协会）</label>                                    </div>                                </div>                            </div>                            <div class="border_bottom_div">                                <div class="border_left_div">                                    <label>申请表</label>                                </div>                                <div class="border_right_div">                                    <label>申根签证申请表</label>                                </div>                            </div>                            <div>                                <div class="border_left_div">                                    <label>邀请函</label>                                </div>                                <div class="border_right_div">                                    <div class="border_bottom_div">                                        <label>商务邀请函(英文)</label>                                    </div>                                    <div class="border_bottom_div">                                        <label>商务邀请函(意大利文)</label>                                    </div>                                    <div class="border_bottom_div">                                        <label>旅游邀请函(英文)</label>                                    </div>                                    <div>                                        <label>旅游邀请函(意大利文)</label>                                    </div>                                </div>                            </div>                        </td>                        <td width="27%">                            <a href="javascript:void(0);" onclick="window.location.href='/visa/download/data?fileName=${goods.visaDataUri!''}'" class="btn_download">所需资料下载</a>                        </td>                    </tr>                </table>            </div>            <div class="z_title" tag_id="2">                <h2>办证流程</h2>            </div>            <div class="z_content">				${goods.visaDescription!''}            </div>            <div class="z_title" tag_id="4">                <h2>费用说明</h2>            </div>            <div class="z_content">				${goods.feeDescription!''}            </div>            <div class="z_title" tag_id="6">                <h2>订购须知</h2>            </div>            <div class="z_content">${goods.bookDescription!''}            </div>        </div>    </div></div><!-- ********************中间内容End******************** --><#include "/client/common_footer.ftl" /></body></html>
